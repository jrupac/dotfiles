URxvt*font: xft:Ubuntu Mono:pixelsize=12
URxvt*background:        #0F0E0F
URxvt*foreground:        White
URxvt.letterSpace:	 -1

# how many lines you want it to save per session
URxvt*saveLines:1000
URxvt*troughColor:#000000
URxvt*scrollColor:#222222

# with this set to true, when an app sends a bell to the terminal, awesome hilights the appropriate tag(s) 
URxvt*urgentOnBell: true

# these three lines enable clicking on links to open them 
## if you want to enable tab support, append ,tabbed to the next line
## then use shift+down arrow to create tabs and shift+{left,right} arrows to switch between them
URxvt*perl-ext-common: default,tabbed,matcher,-option-popup,-selection-popup,-realine
URxvt*matcher.button: 1
# don't forget to change this to your favorite browser
URxvt*urlLauncher: google-chrome

URxvt.tabbed.tabbar-fg: 3
URxvt.tabbed.tabbar-bg: 6
URxvt.tabbed.tab-fg:    3
URxvt.tabbed.tab-bg:    0
URxvt.tabbed.autohide:  yes
URxvt.tabbed.title:  True

! Parker Brothers colors
!black
*color0:    #322a2c
*color8:    #46423b
!red
*color1:    #79220E
*color9:    #AA261F
!green
*color2:    #344B1B
*color10:   #76550F
!yellow
*color3:    #B45B00
*color11:   #B57F02
!blue
*color4:    #434045
*color12:   #908862
!magenta
*color5:    #5C1E25
*color13:   #624A35
!cyan
*color6:    #394745
*color14:   #393E38
!white
*color7:    #AE8842
*color15:   #A59058

Xft.dpi:                    96
Xft.antialias:           true
Xft.hinting:              full
Xft.hintstyle:           hintslight
Xft.rgba:                  rgb

urxvt.termName:          xterm
urxvt.scrollBar:         false
urxvt.imLocale:          en_US.UTF-8
urxvt.loginShell:        true
urxvt.saveLines:         512
urxvt.inheritPixmap:     false
urxvt.shading:           40
urxvt.xftAntialias:      false
urxvt.jumpScroll:        true
urxvt.tintColor:         white
urxvt.internalBorder:    0
urxvt.cursorBlink:       false
urxvt.cursorColor:       #a59461
urxvt.cursorColor2:      #000000
urxvt.colorBD:           #ffffff
urxvt.colorIT:           #bbbbbb
urxvt.colorUL:           #999999
urxvt.underlineColor:    #999999
urxvt.borderless:        true 

